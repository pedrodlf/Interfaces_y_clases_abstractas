    </properties>
    <spring-data xmlns="http://www.netbeans.org/ns/spring-data/1">
        <config-files>
            <config-file>src/main/webapp/WEB-INF/applicationContext.xml</config-file>
            <config-file>src/main/webapp/WEB-INF/dispatcher-servlet.xml</config-file>
